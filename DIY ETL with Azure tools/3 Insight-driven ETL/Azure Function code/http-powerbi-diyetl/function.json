{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "Response"
    },
    {
      "name": "outputBlob",
      "path": "outcontainer/{blobname}",
      "connection": "AzureWebJobsStorage",
      "direction": "out",
      "type": "blob"
    }
  ]
}